@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;

@if (Model.Count > 0)
{
    
    <div class="container">
	<h4 class="green-title">随心搭</h4>
	<ul class="row proList">
        @{int i = 0;
        }
        @foreach (var item in Model)
        {
            i++;
            if(i<=6){
                @Html.Partial("_ProductBox5", item)
            }
           
        }

	</ul>
    </div>
}